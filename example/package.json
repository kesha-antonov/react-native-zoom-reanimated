{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx expo start --dev-client --clear",
    "start:android": "npx expo start --android --dev-client --clear",
    "start:ios": "npx expo start --ios --dev-client --clear",
    "start:web": "npx expo start --web --clear",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fresh": "yarn start --reset-cache",
    "installDevBuild:ios": "npx expo run:ios",
    "installDevBuild:android": "npx expo run:android",
    "rebuildNativeDeps": "npx expo prebuild",
    "rebuildNativeDeps:clean": "npx expo prebuild --clean",
    "freshAndroid": "npx expo prebuild --clean && expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "expo": "~54.0.31",
    "expo-image": "^3.0.11",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-worklets": "~0.7.2",
    "react-native-zoom-reanimated": "link:.."
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@eslint/js": "^9.39.2",
    "@react-native-community/cli": "^20.1.0",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^19.2.8",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "prettier": "^3.8.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "engines": {
    "node": ">=20"
  }
}
