{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx expo start --dev-client --clear",
    "start:android": "npx expo start --android --dev-client --clear",
    "start:ios": "npx expo start --ios --dev-client --clear",
    "start:web": "npx expo start --web --clear",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fresh": "yarn start --reset-cache",
    "installDevBuild:ios": "npx expo run:ios",
    "installDevBuild:android": "npx expo run:android",
    "rebuildNativeDeps": "npx expo prebuild",
    "rebuildNativeDeps:clean": "npx expo prebuild --clean",
    "freshAndroid": "npx expo prebuild --clean && expo run:android",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios"
  },
  "dependencies": {
    "expo": "~54.0.0",
    "expo-status-bar": "~3.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~4.2.0",
    "react-native-worklets": "~0.7.2",
    "react-native-zoom-reanimated": "link:.."
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/js": "^9.17.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.8.0",
    "react-test-renderer": "^19.2.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.33.0"
  },
  "engines": {
    "node": ">=20"
  }
}
